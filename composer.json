{
  "name": "jmfiaschi/zf-filter-afnor",
  "description": "provides a set of AFNOR data filters",
  "license": "MIT",
  "keywords": [
    "zf2",
    "filter",
    "afnor"
  ],
  "homepage": "https://github.com/jmfiaschi/zf-filter-afnor",
  "autoload": {
    "psr-4": {
      "Zend\\Filter\\": "src/"
    }
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "zendframework/zend-filter": "^2.7"
  },
  "require-dev": {
    "zendframework/zend-filter": "^2.7",
    "fabpot/php-cs-fixer": "1.7.*",
    "phpunit/PHPUnit": "~4.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "ZendTest\\Filter\\": "test/"
    }
  }
}